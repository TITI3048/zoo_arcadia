{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 128",
    "not and_ff <= 127",
    "not chrome <= 128",
    "not edge <= 128",
    "not firefox <= 130",
    "not ios_saf <= 17.6",
    "not opera <= 111",
    "not safari <= 17.6",
    "not samsung <= 25"
  ]
}